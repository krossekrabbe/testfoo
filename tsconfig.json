{
  "compilerOptions": {
    "target": "es5",
    "module": "commonjs",
    "lib": [
      "esnext",
      "dom"
    ],
    "strict": true,
    "declaration": true,
    "noImplicitAny": true,
    "noImplicitReturns": true,
    "noUnusedLocals": true,
    "noFallthroughCasesInSwitch": true
  },
  "include": [
    "./lib",
    "cold.ts",
    "index.ts"
  ],
  "exclude": [
    "node_modules"
  ],
"types": [ "node" ],
"typeRoots": [ "./node_modules/@types" ]
}
